{
    "name": "sanmai/pindx",
    "type": "library",
    "description": "Эталонный справочник почтовых индексов объектов почтовой связи",
    "license": "Apache-2.0",
    "require": {
        "php": "^7 || ^8",
        "guzzlehttp/guzzle": "^6.3 || ^7",
        "jms/serializer": "^1.14 || ^2.3 || ^3.0",
        "sanmai/pindx-client": "^0.1"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.8",
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": ">=6",
        "sanmai/pipeline": "^0.3.3 || ^2.0 || ^3.0 || ^5.0",
        "symfony/finder": "^3.4 || ^4.1 || ^5",
        "symfony/yaml": "^3.4 || ^4.3 || ^5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v0.2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "RussianPostIndex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PIndxTools\\": "tools/",
            "Tests\\PIndxTools\\": "tests/"
        }
    }
}
